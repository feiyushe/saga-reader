{
	"productName": "麒睿智库",
	"version": "0.9.6",
	"identifier": "com.sopaco.labs.qinofeed",
	"build": {
		"beforeDevCommand": "bun run dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "bun run build",
		"frontendDist": "../build"
	},
	"app": {
		"windows": [
			{
				"label": "main",
				"title": "麒睿智库",
				"url": "/main",
				"center": true,
				"minWidth": 1440,
				"minHeight": 750
			},
			{
				"label": "reserved",
				"title": "reserved",
				"minWidth": 1440,
				"minHeight": 750,
				"visible": false
			}
		],
		"withGlobalTauri": true,
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"publisher": "Sopaco Global Networks ORG.",
		"copyright": "Sopaco Global Networks ORG.",
		"category": "Productivity",
		"shortDescription": "Qino Feed",
		"longDescription": "A Powerful reader driven by AI Assist-Read, supports multi sources include RSS, Web Search and more.",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"windows": {
			"wix": {
				"language": "zh-CN"
			}
		}
	},
	"plugins": {
		"tauri-plugin-feed-api": {
			"timeout": 30
		}
	}
}
